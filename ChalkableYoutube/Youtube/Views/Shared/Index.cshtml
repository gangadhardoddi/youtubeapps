@model Youtube.Models.StartupViewData

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section headscripts
{
    <script type="text/javascript">
        //--- initialze data for filtering
        var MODE = "@Html.Raw(Model.Mode)" ;
